{
  "name": "json-text-diff",
  "version": "1.0.0",
  "description": "Library for generating diffs for objects for use in assertion libraries",
  "main": "lib/index.js",
  "dependencies": {
    "diff": "^3.5.0",
    "json-stringify-safe": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-json-schema": "^1.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  },
  "scripts": {
    "test": "nyc mocha test/**.js",
